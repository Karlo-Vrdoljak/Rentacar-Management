{
  "name": "rent-a-car",
  "version": "0.0.0",
  "engines": {
    "node": ">=12.18.3"
  },
  "scripts": {
    "build:tailwind": "tailwindcss build -i src/assets/tailwind/tailwind.css -o src/assets/tailwind/tailwind.output.css",
    "build:tailwind:prod": "tailwindcss build -c tailwind.config.prod.js -i src/assets/tailwind/tailwind.css -o src/assets/tailwind/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind:prod",
    "ng": "ng",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --base-href \"/\" --prod --aot --output-hashing=all",
    "watch": "ng build --watch --configuration development",
    "predev:prod": "npm run prebuild",
    "dev:prod": "ng serve --configuration production",
    "test": "ng test",
    "change_theme": "node change.theme.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "dotenv": "^10.0.0",
    "material-icons": "^1.1.1",
    "ngx-form-error": "^1.1.3",
    "ngx-ui-loader": "^11.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "primeflex": "^3.0.1",
    "primeicons": "^4.1.0",
    "primeng": "^12.1.0-rc.1",
    "rxjs": "~6.6.0",
    "tailwindcss": "^2.2.7",
    "tailwindcss-filters": "^3.0.0",
    "tslib": "^2.3.0",
    "typescript-mix": "^3.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.1",
    "@angular/cli": "~12.2.1",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}
